{
  "name": "express-for-starters",
  "version": "1.0.0",
  "description": "Express starter template",
  "main": "dist/src/index.js",
  "scripts": {
    "watch": "tsc -w",
    "start": "node ",
    "build": "tsc",
    "dev": "cross-env DEBUG=express-for-starters:* nodemon dist/index.js",
    "dev2": "cross-env QUEUE_ID=1 DEBUG=express-for-starters:* nodemon dist/index.js",
    "dev:ts": "cross-env DEBUG=express-for-starters:*  nodemon --exec ts-node src",
    "types:check": "tsc --noEmit"
  },
  "keywords": [
    "node",
    "typescript",
    "express"
  ],
  "author": "Domey Benjamin<domeybenjamin1@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/debug": "4.1.8",
    "@types/express": "4.17.17",
    "@types/jsonwebtoken": "9.0.2",
    "@types/uuid": "^9.0.2",
    "cross-env": "7.0.3",
    "nodemon": "2.0.22",
    "ts-node": "10.9.1",
    "typescript": "5.1.3"
  },
  "dependencies": {
    "@types/node": "20.2.5",
    "cors": "2.8.5",
    "debug": "4.3.4",
    "dotenv": "16.1.4",
    "express": "4.18.2",
    "uuid": "^9.0.0"
  }
}
